package com.adapter;

public class Adapter implements Engineer{
	//private GoblinGlider adaptee;
	public Adapter(){
	 GoblinGlider adaptee = new GoblinGlider();
	}
	
	@Override
	public void operateDevice() {
		// TODO Auto-generated method stub
		adaptee.start();
		adaptee.run();
		adaptee.stop();
	}

}
