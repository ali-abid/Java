package com.adapter;
/*
 * Author Abid Ali
 * Design Pattern
 * Adapter
 * 
 */
public class Manager implements Engineer{

	private Engineer engineer;
	public Manager(){
		engineer = new Adapter();
	}
	
	@Override
	public void operateDevice() {
		// TODO Auto-generated method stub
		System.out.println("Engineer is ready");
		engineer.operateDevice();
		
	}

}
